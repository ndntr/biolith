{
  "name": "saltpile-engine",
  "version": "1.0.0",
  "description": "EvidenceAlerts email ingestion and processing engine for Biolith",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node dist/index.js",
    "process": "npm run build && node dist/index.js",
    "test:local": "npm run build && node dist/index.js --test-local",
    "test:fetch": "npm run build && node dist/index.js --test-fetch",
    "test:parse": "npm run build && node dist/index.js --test-parse",
    "test:scrape": "npm run build && node dist/index.js --test-scrape",
    "local": "npm run build && npm run test:local"
  },
  "keywords": [
    "evidence",
    "medical",
    "email",
    "processing",
    "biolith"
  ],
  "author": "Biolith",
  "license": "MIT",
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "axios": "^1.11.0",
    "cheerio": "^1.0.0-rc.12",
    "crypto": "^1.0.1",
    "imap": "^0.8.19",
    "mailparser": "^3.7.1",
    "node-fetch": "^3.3.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/imap": "^0.8.40",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
